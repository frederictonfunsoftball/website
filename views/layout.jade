doctype html
html
  head
    title Fredericton Co-Ed Fun Softball League
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css')
    block scripts
      script(src="//ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js")
      script(src="/javascripts/commonFunctions.js")
      script(src="/javascripts/moment.js")
      //https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.1/moment.min.js
      script(src="//code.jquery.com/ui/1.11.4/jquery-ui.js")
</script>
  body
    div#header
      div#headerContents
        include ./header.jade
    div#body
      div#bodyContents
        div#leftPanel
          include ./leftPanel.jade
        div#rightPanel
          include ./rightPanel.jade
        div#content
          block content

    div#footer
      include ./footer.jade
